FLASK_APP=app.py
FLASK_DEBUG=1
SECRET_KEY=change-me
DATABASE_URL=mysql+pymysql://user:password@127.0.0.1:3306/dublinbikes

# JWT 认证（access 与 refresh 使用不同密钥）
JWT_SECRET_KEY=your-jwt-access-secret-key-change-in-production
JWT_REFRESH_SECRET_KEY=your-jwt-refresh-secret-key-change-in-production
# 可选：access 过期时间（秒），默认 900（15 分钟）
# JWT_ACCESS_EXPIRES_SECONDS=900
# 可选：refresh 过期时间（秒），默认 604800（7 天）
# JWT_REFRESH_EXPIRES_SECONDS=604800
